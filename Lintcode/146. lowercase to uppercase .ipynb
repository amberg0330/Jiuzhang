   "source": [
    "def lowercaseToUppercase2(str):\n",
    "    out = ''\n",
    "    for i in range(len(str)):\n",
    "        if ord('a')<=ord(str[i])<=ord('z'):\n",
    "            out += chr(ord(str[i])+(ord('A')-ord('a')))\n",
    "        else:\n",
    "            out += str[i]\n",
    "    return out\n",
    "    "
   ]
